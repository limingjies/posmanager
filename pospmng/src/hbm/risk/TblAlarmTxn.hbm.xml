<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.allinfinance.po.risk">
	<class
		name="TblAlarmTxn"
		table="tbl_alarm_txn"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="tblAlarmTxnPk" class="TblAlarmTxnPK">
			<key-property
				name="alarmId"
				column="alarm_id"
				type="string"
			/>
			
			<key-property
				name="alarmSeq"
				column="alarm_seq"
				type="integer"
			/>
	
			<key-property
				name="instDate"
				column="inst_date"
				type="string"
			/>
			<key-property
				name="sysSeqNum"
				column="sys_seq_num"
				type="string"
			/>
		</composite-id>
		<property
			name="cardAccpId"
			column="card_accp_id"
			type="string"
			not-null="true"
			length="15"
		/>
		<property
			name="pan"
			column="pan"
			type="string"
			not-null="true"
			length="19"
		/>
		<property
			name="cardAccpTermId"
			column="card_accp_term_id"
			type="string"
			not-null="true"
			length="8"
		/>
		<property
			name="txnNum"
			column="txn_num"
			type="string"
			not-null="true"
			length="4"
		/>
		<property
			name="amtTrans"
			column="amt_trans"
			type="string"
			not-null="true"
			length="12"
		/>
		<property
			name="transState"
			column="trans_state"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="rspCode"
			column="rsp_code"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="cheatTp"
			column="cheat_tp"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="cheatFlag"
			column="cheat_flag"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="cautionFlag"
			column="caution_flag"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="receiptFlag"
			column="receipt_flag"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="blockFlag"
			column="block_flag"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="misc"
			column="misc"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="misc1"
			column="misc1"
			type="string"
			not-null="false"
			length="200"
		/>
	</class>	
</hibernate-mapping>