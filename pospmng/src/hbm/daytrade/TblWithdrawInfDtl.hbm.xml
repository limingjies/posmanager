<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.allinfinance.po.daytrade">
	<class
		name="TblWithdrawInfDtl"
		table="TBL_WITHDRAW_INF_DTL"
	>
		<meta attribute="sync-DAO">false</meta>

		<composite-id>
			<key-property
				name="batchNo" column="BATCH_NO" type="string" length="28"
			/>
			<key-property
				name="instDate" column="INST_DATE" type="string" length="14" 
			/>
			<key-property
				name="sysSeqNum" column="SYS_SEQ_NUM" type="string" length="6" 
			/>
		</composite-id>
	</class>	
</hibernate-mapping>