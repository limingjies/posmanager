			name="frzAmt"
			column="FRZ_AMT"
			type="string"
			not-null="false"
			length="13"
			/>
		<property
			name="flag1"
			column="FLAG1"
			type="string"
			not-null="false"
			length="20"
			/>
		<property
			name="flag2"
			column="FLAG2"
			type="string"
			not-null="false"
			length="20"
			/>
		<property
			name="flag3"
			column="FLAG3"
			type="string"
			not-null="false"
			length="20"
			/>
		<property
			name="misc1"
