			type="string"
			not-null="false"
			length="128"
			/>
		<property
			name="misc2"
			column="MISC2"
			type="string"
			not-null="false"
			length="128"
			/>
		<property
			name="misc3"
			column="MISC3"
			type="string"
			not-null="false"
			length="128"
			/>
		<property
			name="lstUpdTlr"
			column="LST_UPD_TLR"
			type="string"
			not-null="false"
			length="11"
			/>
		<property
			name="lstUpdTime"
			column="LST_UPD_TIME"
			type="string"
			not-null="false"
			length="14"
			/>
		<property
			name="createTime"
			column="CREATE_TIME"
			type="string"
			not-null="false"
			length="14"
			/>
	</class>	
</hibernate-mapping>