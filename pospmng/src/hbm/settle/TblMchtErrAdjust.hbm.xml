<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.allinfinance.po.settle">
	<class
		name="TblMchtErrAdjust"
		table="TBL_MCHT_ERR_ADJUST"
	>
		<id name="id" 
			type="string" 
			column="UUID"
			length="36"
		>
			<generator class="assigned" />
		</id>
		
		
		<property
			name="mchtNo"
			column="MCHT_NO"
			type="string"
			not-null="true"
			length="15"
		/>
		
		
		<property
			name="cardNo"
			column="CARD_NO"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="authNo"
			column="AUTH_NO"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="tradeConsultNo"
			column="TRADE_CONSULT_NO"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="tradeType"
			column="TRADE_TYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="money"
			column="MONEY"
			type="java.lang.Double"
			not-null="false"
			length="12"
			precision="15" scale="2"
		/>
		<property
			name="approveStatus"
			column="APROVE_STATUS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="approveOpr"
			column="APROVE_OPR"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="approveTime"
			column="APPROVE_TIME"
			type="string"
			not-null="false"
			length="14"
		/>
		<property
			name="approveAdvice"
			column="APPROVE_ADVICE"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="postTime"
			column="POST_TIME"
			type="string"
			not-null="false"
			length="14"
		/>
		<property
			name="postStatus"
			column="POST_STATUS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="reserver"
			column="RESERVER"
			type="string"
			not-null="false"
			length="252"
		/>
		<property
			name="crtTime"
			column="CRT_TIME"
			type="string"
			not-null="false"
			length="14"
		/>
		<property
			name="crtOpr"
			column="CRT_OPR"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="updTime"
			column="UPD_TIME"
			type="string"
			not-null="false"
			length="14"
		/>
		<property
			name="updOpr"
			column="UPD_OPR"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="vcTranNo"
			column="VC_TRAN_NO"
			type="string"
			not-null="false"
			length="10"
		/>
	</class>	
</hibernate-mapping>