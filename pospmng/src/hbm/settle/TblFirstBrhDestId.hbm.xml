<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.allinfinance.po.settle">
	<class
		name="TblFirstBrhDestId"
		table="TBL_FIRST_BRH_DEST_ID"
	>
		<meta attribute="sync-DAO">false</meta>

		<id
			name="destId"
			column="DEST_ID"
			type="string"
			length="4"
		/>
		<property name="firstBrhId" column="FIRST_BRH_ID" type="string" not-null="false" length="4" />
		<property name="firstBrhName" column="FIRST_BRH_NAME" type="string" not-null="false" length="100" />
		<property name="bak1" column="BAK1" type="string" not-null="false" length="10" />
		<property name="bak2" column="BAK2" type="string" not-null="false" length="10" />
		<property name="bak3" column="BAK3" type="string" not-null="false" length="10" />
		<property name="bak4" column="BAK4" type="string" not-null="false" length="100" />


	</class>	
</hibernate-mapping>