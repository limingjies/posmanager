			<key-property
				name="channelId"
				column="CHANNEL_ID"
				type="string"
			/>
		</composite-id>

		<property
			name="begDate"
			column="beg_date"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="endDate"
			column="end_date"
			type="string"
			not-null="false"
			length="8"
		/>
