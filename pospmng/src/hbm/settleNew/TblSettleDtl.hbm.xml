			name="amtErrMinus"
			column="amt_err_minus"
			type="big_decimal"
			not-null="false"
			length="15"
		/>
		<property
			name="amtErrAdd"
			column="amt_err_add"
			type="big_decimal"
			not-null="false"
			length="15"
		/>
		<property
			name="amtHanging"
			column="amt_hanging"
			type="big_decimal"
			not-null="false"
			length="15"
		/>
		
		<property
			name="amtSettle"
			column="amt_settle"
			type="big_decimal"
			not-null="false"
			length="15"
		/>
		
		<property
			name="payType"
			column="PAY_TYPE"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="mchtNm"
			column="mcht_nm"
			type="string"
			not-null="false"
