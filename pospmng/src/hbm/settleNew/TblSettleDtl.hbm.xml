		<property
			name="cnapsId"
			column="CNAPS_ID"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="cnapsName"
			column="CNAPS_NAME"
			type="string"
			not-null="false"
			length="256"
		/>
		
		
		<property
			name="areaNm"
			column="area_nm"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="reserved1"
