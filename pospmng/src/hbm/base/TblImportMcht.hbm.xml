<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.allinfinance.po.base">
	<class
		name="TblImportMcht"
		table="TBL_IMPORT_MCHT"
	>
       <id
			name="orgId"
			type="string"
			column="ORG_ID"
			length="15" 
		>
			<generator class="assigned"/>
		</id>
		<property
			name="newId"
			column="NEW_ID"
			type="string"
			not-null="true"
			length="15"
		/>
		<property
			name="orderNo"
			column="ORDER_NO"
			type="string"
			not-null="true"
			length="15"
		/>
		<property
			name="picFlag"
			column="PIC_FLAG"
			type="string"
			length="1"
		/>
		


	</class>	
</hibernate-mapping>